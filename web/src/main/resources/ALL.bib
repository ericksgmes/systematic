@ARTICLE{Chen2010b,
  author = {Chen, Lu and Chow, Randy},
  title = {A web service similarity refinement framework using automata comparison},
  journal = {Communications in Computer and Information Science},
  year = {2010},
  volume = {54},
  pages = {44 - 55},
  note = {Comparison methods;Composite services;Composite Web services;Control
	constructs;Internal structure;Key issues;Measuring method;Nondeterministic
	finite automaton;Regular languages;Similarity measure;Solution approach;Two
	stage;Wordnet;},
  abstract = {A key issue in web service based technology is finding an effective
	web service similarity comparison method. Current solution approaches
	include similarities comparisons both on signature level and behavior
	level. The accuracy of comparison is generally satisfactory if the
	web services are atomic. However, for composite web services with
	complicated internal structures, the precision of similarity results
	depends on the comparison approaches. To enhance this precision for
	composite services, the paper proposes a two-stage framework to integrate
	the signature level and behavior level similarities. The first stage
	adopts the traditional WordNet-based interface measuring method to
	generate a set of possible similar services. Then in the second stage,
	an automaton is built for each service. By characterizing and comparing
	the languages generated from those automata, we can enhance the precision
	of similarity measures of similar services from the previous stage.
	&copy; 2010 Springer-Verlag Berlin Heidelberg.},
  address = {Tiergartenstrasse 17, Heidelberg, D-69121, Germany},
  copyright = {Compilation and indexing terms, Copyright 2011 Elsevier Inc.},
  issn = {18650929},
  key = {Web services},
  keywords = {Finite automata;Linguistics;Robots;Translation (languages);},
  language = {English},
  source = {Scopus, Compendex},
  url = {http://dx.doi.org/10.1007/978-3-642-12035-0_6}
}

