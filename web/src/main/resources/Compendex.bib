}

@ARTICLE{Frei2010,
  author = {Frei, Regina and di Marzo Serugendo, Giovanna and Serbanuta, Traian
	Florin},
  title = {Ambient intelligence in self-organising assembly systems using the
	chemical reaction model},
  journal = {Journal of Ambient Intelligence and Humanized Computing},
  year = {2010},
  volume = {1},
  pages = {163 - 184},
  number = {3},
  note = {Ambient intelligence;Application domains;Assembly systems;Chemical
	abstract machines;Error prones;Evolvable;Manufacturing system;Market
	dynamics;Product orders;Self-assemble;Self-organisation;Self-organising;},
  abstract = {This article discusses self-organising assembly systems (SOAS), a
	type of assembly systems that (1) participate in their own design
	by spontaneously organising themselves in response to the arrival
	of a product order and (2) manage themselves during production. SOAS
	address the industry's need for agile manufacturing systems to be
	highly responsive to market dynamics. Manufacturing systems need
	to be easily and rapidly changeable, but system re-engineering/reconfiguration
	and especially their (re-)programming are manual, work-intensive
	and errorprone procedures. With SOAS, we try to facilitate this by
	giving the systems gradually more self-* capabilities. SOAS eases
	the work of the SOAS designer and engineer when designing such as
	system for a specific product, and supports the work of the SOAS
	operator when supervising the system during production. SOAS represent
	an application domain of ambient intelligence and humanised computing
	which is not frequently considered, but therefore none the less important.
	This article explains how an SOAS produces its own design as the
	result of a self-organising process following the Chemical Abstract
	Machine (CHAM) paradigm: industrial robots self-assemble according
	to specific chemical rules in response to a product order. This paper
	reports on SOAS in general, the specification of the chemical reactions
	and their simulation in Maude. &copy; Springer-Verlag 2010.},
  address = {Tiergartenstrasse 17, Heidelberg, D-69121, Germany},
  copyright = {Compilation and indexing terms, Copyright 2011 Elsevier Inc.},
  issn = {18685137},
  key = {Assembly},
  keywords = {Agile manufacturing systems;Artificial intelligence;Chemical reactions;Customer
	satisfaction;Industrial robots;Machine design;Riveting;},
  language = {English},
  source = {Compendex},
  url = {http://dx.doi.org/10.1007/s12652-010-0016-0}
}

