@INPROCEEDINGS{Bi2008,
  author = {Bi, Luyan and Jiao, Zongxia},
  title = {An information integration framework based on XML to support mechatronics
	multi-disciplinary design},
  booktitle = {2008 IEEE International Conference on Robotics, Automation and Mechatronics,
