@ARTICLE{Witwicki2009,
  author = {Witwicki, Stefan J. and Durfee, Edmund H.},
  title = {Commitment-based service coordination},
  journal = {International Journal of Agent-Oriented Software Engineering},
  year = {2009},
  volume = {3},
  pages = {59 - 87},
  number = {1},
  note = {Commitments;Heuristic search;Large-grained services;Markov Decision
	Processes;Service oriented computing;},
  abstract = {We present a methodology for composing large-grained services that
	exhibit temporal uncertainty and complex task dependencies. Our multi-agent
	approach incorporates temporal and stochastic planning paradigms
	and commitment-based negotiation to achieve the coordinated provision
	of services with stochastic outcomes. This is all captured within
	a service-choreography protocol, by which agents can request future
	service provisions and receive probabilistic temporal service promises
	in return, converging on coordinated decisions about how and when
	to work together. We hypothesise that our approach can scalably converge
