@INPROCEEDINGS{Raimondi2008,
  author = {Raimondi, Franco and Skene, James and Emmerich, Wolfgang},
  title = {Efficient online monitoring of web-service SLAs},
  booktitle = {Proceedings of the ACM SIGSOFT Symposium on the Foundations of Software
	Engineering},
  year = {2008},
  pages = {170 - 180},
  address = {Atlanta, GA, United states},
  note = {Computational grids;Eclipse plugin;Linear-run;On-line monitors;Online
	monitoring;Penalty payment;Quadratic time;Runtimes;Service Level
	Agreements;Service Oriented;Service Quality;Timed Automata;},
  abstract = {If an organization depends on the service quality provided by another
	organization it often enters into a bilateral service level agreement
	(SLA), which mitigates outsourcing risks by associating penalty payments
	with poor service quality. Once these agreements are entered into,
	it becomes necessary to monitor their conditions, which will commonly
	relate to timeliness, reliability and request throughput, at run-time.
	We show how these conditions can be translated into timed automata.
	Acceptance of a timed word by a timed automaton can be decided in
	quadratic time and because the timed automata can operate while messages
	are exchanged at run-time there is effectively only a linear run-time
	overhead. We present an implementation to derive on-line monitors
	for web services automatically from SLAs using an Eclipse plugin.
	We evaluate the efficiency and scalability of this approach using
	a large-scale case study in a service-oriented computational grid.
	&copy; 2008 ACM.},
  copyright = {Compilation and indexing terms, Copyright 2011 Elsevier Inc.},
  key = {Quality of service},
  keywords = {Automata theory;Computer software;Network architecture;Robots;Translation
	(languages);Voltage measurement;},
  language = {English},
